/* pages/QRCode/QRCode.wxss */
.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 20rpx 30rpx 0 30rpx;
}
.container .button {
  width: 100%;
  letter-spacing: 5rpx;
  border: none;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
  background-color: #FFFFFF;
  box-shadow: 2rpx 4rpx 12rpx 2rpx rgba(0, 0, 0, 0.16);
}
.container .cellTitle {
  color: #969799;
}
.container .goodsItem {
  width: 100%;
  display: flex;
  align-items: flex-start;
  justify-content: flex-start;
  background-color: #FFFFFF;
  padding: 0 20rpx 20rpx 20rpx;
}
.container .goodsItem .image {
  width: 100rpx;
  height: 100rpx;
}
.container .goodsItem .title {
  font-size: 28rpx;
  margin-left: 30rpx;
}
.container .delete {
  width: 120rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FFFFFF;
  background-color: #DB211B;
}
